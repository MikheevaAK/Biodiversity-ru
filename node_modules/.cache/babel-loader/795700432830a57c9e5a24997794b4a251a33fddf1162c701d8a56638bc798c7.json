{"ast":null,"code":"import gsap from \"gsap\";\nimport ScrollTrigger from \"gsap/ScrollTrigger\";\nimport BaseTextBlock from '@/components/BaseTextBlock.vue';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  mounted() {\n    this.$nextTick(function () {\n      this.scrollAnimation();\n    });\n  },\n  components: {\n    BaseTextBlock\n  },\n  methods: {\n    scrollAnimation() {\n      if (window.innerWidth > 768) {\n        const panels = gsap.utils.toArray(\".participants-container .participants-item\");\n        gsap.to(panels, {\n          xPercent: -70 * (panels.length - 1),\n          ease: \"none\",\n          scrollTrigger: {\n            trigger: \".participants-container__wrap\",\n            pin: true,\n            start: \"top 10%\",\n            scrub: 1,\n            end: () => \"+=\" + (document.querySelector(\".participants-container\").offsetWidth - window.innerWidth)\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["gsap","ScrollTrigger","BaseTextBlock","registerPlugin","mounted","$nextTick","scrollAnimation","components","methods","window","innerWidth","panels","utils","toArray","to","xPercent","length","ease","scrollTrigger","trigger","pin","start","scrub","end","document","querySelector","offsetWidth"],"sources":["src/components/ParticipantsSwiper.vue"],"sourcesContent":["<template>\n    <div class=\"participants-container__wrap\">\n        <BaseTextBlock class=\"mb-100\" title=\"Участники\">\n            <span>\n                Проект носит междисциплинарный и&nbsp;кросс-культурный характер. В&nbsp;проекте\n                и&nbsp;научно-просветительской программе приняли участие художественные галереи, музеи\n                и&nbsp;природные заповедники.\n            </span>\n        </BaseTextBlock>\n        <div class=\"participants-container\">\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-1.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    Музейное объединение «Художественная культура Русского Севера»\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-2.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    «Северный морской музей», Архангельск\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-3.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    «Таймырский краеведческий музей», Дудинка\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-4.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    «Таймырский Дом народного творчества», Дудинка\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-5.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    «Краевой краеведческий музей», Красноярск\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-6.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    Художественная галерея «Музей Норильска», Норильск\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-7.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    «Российский государственный музей Арктики и Антарктики», Санкт-Петербург\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-8.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    Сибирское отделение РАН, Новосибирск\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-9.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    Институт океанологии РАН им.&nbsp;П.&nbsp;П.&nbsp;Ширшова, Москва\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-10.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    Государственный Дарвиновский музей, Москва\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-11.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    Заповедник «Лапландский», Мончегорск\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-12.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    Заповедник &laquo;Пасвик&raquo;, пгт&nbsp;Никель\n                </div>\n            </div>\n\n            <div class=\"participants-item\">\n                <img src=\"img/art/participants/participants-13.svg\" alt=\"\">\n                <div class=\"participants-item__title\">\n                    Национальный парк «Красноярские столбы», Красноярск\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport gsap from \"gsap\";\nimport ScrollTrigger from \"gsap/ScrollTrigger\";\nimport BaseTextBlock from '@/components/BaseTextBlock.vue'\n\ngsap.registerPlugin(ScrollTrigger);\n\n\nexport default {\n    mounted() {\n        this.$nextTick(function () {\n            this.scrollAnimation();\n        })\n    },\n    components: {\n        BaseTextBlock,\n    },\n    methods: {\n        scrollAnimation() {\n            if (window.innerWidth > 768) {\n                const panels = gsap.utils.toArray(\".participants-container .participants-item\");\n                gsap.to(panels, {\n                    xPercent: -70 * (panels.length - 1),\n                    ease: \"none\",\n                    scrollTrigger: {\n                        trigger: \".participants-container__wrap\",\n                        pin: true,\n                        start: \"top 10%\",\n                        scrub: 1,\n                        end: () => \"+=\" + (document.querySelector(\".participants-container\").offsetWidth - window.innerWidth)\n                    }\n                });\n            }\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\">\n.participants {\n    position: relative;\n\n    &__wrap {\n        overflow: hidden;\n    }\n\n    .mobile-icon-scroll {\n        display: none;\n        position: absolute;\n        width: 12.8rem;\n        height: 11.2rem;\n        top: -20rem;\n        right: 7rem;\n\n        @media (max-width: 768px) {\n            display: block;\n        }\n    }\n\n    &-container {\n        margin-left: 3.69rem;\n        margin-bottom: 4.94rem;\n        display: flex;\n        gap: 2.125rem;\n        width: 220rem;\n        // height: 30.139rem;\n\n        @media (max-width: 768px) {\n            width: 500rem;\n            height: 45.34rem;\n            gap: 17.07rem;\n            margin-left: 3.2rem;\n            margin-bottom: 0;\n\n            &__wrap {\n                margin-bottom: 16rem;\n                width: unset;\n                overflow-x: scroll;\n                overflow-y: hidden;\n                scrollbar-width: none;\n\n                &::-webkit-scrollbar {\n                    width: 0;\n                }\n            }\n        }\n    }\n\n}\n\n.participants-item {\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n    width: 13.6875rem;\n\n    @media (max-width: 768px) {\n        width: 85.07rem;\n        align-items: center;\n    }\n\n    img {\n        width: auto;\n        height: 9.5625rem;\n        margin-bottom: 1rem;\n\n        @media (max-width: 768px) {\n            width: 32.54rem;\n            margin-bottom: 8.8rem;\n        }\n    }\n\n    &__title {\n        font-size: 0.9375rem;\n        height: 5.5rem;\n        font-weight: 600;\n        line-height: 130%;\n        @media (max-width: 768px) {\n            text-align: center;\n            margin-bottom: 5.07rem;\n            font-size: 4.8rem;\n        }\n    }\n}\n</style>"],"mappings":"AAyGA,OAAAA,IAAA;AACA,OAAAC,aAAA;AACA,OAAAC,aAAA;AAEAF,IAAA,CAAAG,cAAA,CAAAF,aAAA;AAGA;EACAG,QAAA;IACA,KAAAC,SAAA;MACA,KAAAC,eAAA;IACA;EACA;EACAC,UAAA;IACAL;EACA;EACAM,OAAA;IACAF,gBAAA;MACA,IAAAG,MAAA,CAAAC,UAAA;QACA,MAAAC,MAAA,GAAAX,IAAA,CAAAY,KAAA,CAAAC,OAAA;QACAb,IAAA,CAAAc,EAAA,CAAAH,MAAA;UACAI,QAAA,SAAAJ,MAAA,CAAAK,MAAA;UACAC,IAAA;UACAC,aAAA;YACAC,OAAA;YACAC,GAAA;YACAC,KAAA;YACAC,KAAA;YACAC,GAAA,EAAAA,CAAA,aAAAC,QAAA,CAAAC,aAAA,4BAAAC,WAAA,GAAAjB,MAAA,CAAAC,UAAA;UACA;QACA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}